  "keyFeatures": [
    "Real-time pose scoring with MediaPipe",
    "AI coaching with OpenAI GPT-4o-mini and TTS",
    "Automatic dance/gym mode detection",
    "Gesture controls (raise hands to pause/play)",
    "Person segmentation with SAM2",
    "AI video generation from text prompts",
    "Spotify Wrapped-style performance reports",
    "Chrome extension for YouTube integration",
    "Zoom app for meeting-based coaching"
  ],
  "publicApiSurface": [
    "/api/download - Download YouTube videos with SSE progress",
    "/api/video/[id] - Serve videos with range request support",
    "/api/coach - OpenAI coaching with TTS audio",
    "/api/report - Generate performance report cards",
    "/api/score - Real-time pose scoring with Groq vision",
    "/api/segment - Video segmentation with SAM2",
    "/api/generate - AI video generation with Grok",
    "CameraPanel - Webcam feed with pose overlay",
    "YoutubePanel - Video player with custom controls",
    "CoachPanel - AI coaching feedback display",
    "ScorePopup - Real-time score notifications",
    "ReportCard - Performance summary with grades",
    "loadPose() - Initialize MediaPipe Pose",
    "computeScore() - Calculate pose match score",
    "buildPoseSummary() - Generate coaching context",
    "extractPoses() - Extract pose timeline from video"
  ]
